<Page x:Class="test8.login"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:test8"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="login">

    <Grid Background="White" Margin="0,0,303,0">
        <Label Content="username" HorizontalAlignment="Left" Margin="46,94,0,0" VerticalAlignment="Top" Height="51" Width="132" FontSize="20" FontFamily="Arial Black"/>
        <Label Content="password" HorizontalAlignment="Left" Margin="46,202,0,0" VerticalAlignment="Top" Height="51" Width="132" FontSize="20" FontFamily="Arial Black"/>
        <TextBox x:Name="user" HorizontalAlignment="Left" Margin="274,102,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="126" Height="43"/>
        <TextBox x:Name="pass" HorizontalAlignment="Left" Margin="274,210,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="126" Height="43"/>
        <Button x:Name="log" Content="login" HorizontalAlignment="Left" Margin="203,341,0,0" VerticalAlignment="Top" Height="39" Width="134" FontFamily="Broadway" FontSize="20" Click="log_Click"/>

    </Grid>
</Page>
